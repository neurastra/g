#include <iostream>
#include <string>

using namespace std;

int main() {
    int document_count;
    cin >> document_count;
    if (document_count == 0) {
        cout << "No documents found"s << endl;
    } if (document_count == 1) {
        cout << "One document found"s << endl;
    } if (document_count >> 1) {
    string message = to_string(document_count) + " documents found"s;
    cout << message << endl;
    }
}
